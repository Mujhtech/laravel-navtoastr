{
    "name": "mujhtech/nav-toastr",
    "description": "PHP API Library for varspay service",
    "keywords": ["laravel", "toastr", "php", "navigation", "redirection", "session", "route", "library"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mujhtech",
            "email": "mujeeb.muhideen@gmail.com"
        }
    ],
    "require": {
        "laravel/framework": ">=6.0",
        "php": ">=7.2.0",
        "illuminate/support": "~6|~7|~8"
    },
    "autoload": {
        "files": [
            "src/Helpers.php"
          ],
        "psr-4": {
            "Mujhtech\\NavToastr\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mujhtech\\NavToastr\\NavToastrServiceProvider"
            ],
            "aliases": {
                "NavToastr": "Mujhtech\\NavToastr\\Facades\\NavToastr"
            }
        }
    }
}
