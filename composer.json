{
    "name": "mujhtech/nav-toastr",
    "description": "nav-toastr flash notification with custom redirection for laravel app",
    "keywords": ["laravel", "toastr", "toast", "flast", "flash notification", "mujhtech", "php", "navigation", "redirection", "session", "route", "library"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mujhtech",
            "email": "mujeeb.muhideen@gmail.com",
            "homepage": "https://twitter.com/MMujhtech",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": ">=6.0",
        "php": ">=7.2.0",
        "illuminate/support": "~6|~7|~8"
    },
    "autoload": {
        "psr-4": {
            "Mujhtech\\NavToastr\\": "src/"
        },
        "files": [
            "src/Helpers.php"
        ]
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Mujhtech\\NavToastr\\NavToastrServiceProvider"
            ],
            "aliases": {
                "NavToastr": "Mujhtech\\NavToastr\\Facades\\NavToastr"
            }
        }
    }
}
